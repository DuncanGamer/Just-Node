<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Usuario</h1>
    <table>
        <tr>
            <th>ID</th>
            <th>Nombre</th>
            <th>Edad</th>
        </tr>
       <% users.forEach((user) => {%>
            <tr>
                <td> <%= user.id %> </td>
                <td> <%= user.name %> </td>
                <td> <%= user.age %> </td>
            </tr>
       <% }) %>
    </table>
</body>
</html>